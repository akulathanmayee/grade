#include<stdio.h>
int main()
{
	int a;
	printf("Enter the marks= ");
	scanf("%d", &a);
	
 if(a>=85) {
printf("Grade A");
}
else if(a<85 && a>=70) 
{
printf("Grade B");
}
else if(a<70 && a>=55)
{
printf("Grade C");
}
else if(a<55 && a>=40) 
{
printf("Grade D");
}
else 
{
printf("Fail");
}
return 0;
}
